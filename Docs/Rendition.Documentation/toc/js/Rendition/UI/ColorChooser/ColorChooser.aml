	<!-- ============================== Examples ========================== -->	
		
			<section address="examples">
				<title>Examples</title>
				<sections>
					
						<section>
							<title>
								
									 Bind a color chooser to an input 

								
							</title>
							<content>
								<para>
									<code language="javascript">
var foo = Rendition.UI.Dialog();
var bar = document.createElement('input');
foo.content.appendChild(bar);
var unus = Rendition.UI.ColorChooser({
	input: bar,
	callbackProcedure: function (hexColor, hexColorWithPoundPrefix, colorChooser) {
		bar.value = hexColor;
	}
});</code>
								</para>
							</content>
						</section>
					
				</sections>
			</section>
		
